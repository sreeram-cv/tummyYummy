<%= form_for(@agent) do |f| %>
  <% if @agent.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@agent.errors.count, "error") %> prohibited this agent from being saved:</h2>

      <ul>
      <% @agent.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <h2 class="fs-title">Create your account</h2>
  <div class="field">
    <%= f.text_field :name ,placeholder:"Name" %>
  </div>
  <div class="field">
    <%= f.text_field :email ,placeholder:"Email"%>
  </div>
  <div class="field">
    <%= f.text_field :password ,placeholder:"Password"%>
  </div>
  <div class="field">
    <%= f.text_field :address ,placeholder:"Address"%>
  </div>
  <div class="field">
    <%= f.text_field :phone ,placeholder:"Phone"%>
  </div>
  <div class="field">
    <%= f.text_area :items ,placeholder:"Enter coma separated food menu"%>
  </div>
  <div class="actions action-button">
    <%= f.submit %>
  </div>
<% end %>
